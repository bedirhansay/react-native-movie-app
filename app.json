{
  "expo": {
    "name": "mersel",
    "slug": "mersel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/mersel-logo.png",
    "scheme": "mersel",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bedirhansay.mersel",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/mersel-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.bedirhansay.mersel"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/mersel-logo.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#232323"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7003486b-52c8-4eab-893f-3bf63fdf20c7"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7003486b-52c8-4eab-893f-3bf63fdf20c7"
    },
    "owner": "bedirhansay"
  }
}
